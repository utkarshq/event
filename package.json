{
  "name": "vision-event-engine",
  "version": "1.0.0",
  "description": "Vision-first document extraction engine with multi-provider AI support",
  "author": "Vision Event Engine Team",
  "license": "UNLICENSED",
  "private": true,
  "type": "module",
  "module": "src/server.ts",
  "scripts": {
    "start": "bun src/server.ts",
    "dev": "bun --watch src/server.ts",
    "typecheck": "bunx tsc --noEmit",
    "clean": "rm -rf node_modules .venv .paddle_cache data/events.sqlite",
    "docker:build": "docker compose build",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:dev": "docker compose -f docker-compose.dev.yml up"
  },
  "devDependencies": {
    "@types/bun": "^1.3.6"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "keywords": [
    "ocr",
    "document-extraction",
    "vision-ai",
    "paddleocr",
    "gemini",
    "openai"
  ]
}